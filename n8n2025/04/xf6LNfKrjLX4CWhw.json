    {
      "parameters": {
        "jsCode": "const rootDomain = \"simpleit.app.br\";\n\nreturn items\n  .filter(item => item.json.result) // filtra só os que têm \"result\"\n  .flatMap(item => item.json.result) // espalha os registros DNS\n  .filter(record => record.name && record.name !== rootDomain) // ignora domínio raiz\n  .map(record => {\n    const full = record.name;\n    const subdomain = full.endsWith(`.${rootDomain}`)\n      ? full.replace(`.${rootDomain}`, \"\")\n      : full;\n    return { json: { subdomain } };\n  });\n"
